<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sensetime.senseme.com.effects">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Light.NoTitleBar">
        <activity
            android:name="sensetime.senseme.com.effects.WelcomeActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="sensetime.senseme.com.effects.CameraActivityNew"
            android:screenOrientation="portrait"/>
        <activity
            android:name="sensetime.senseme.com.effects.ImageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="sensetime.senseme.com.effects.CpuFilterActivity"
            android:screenOrientation="portrait" />
       <activity android:name="sensetime.senseme.com.effects.CameraActivity">
           <intent-filter>
               <action android:name="android.intent.action.MAIN" />

               <category android:name="android.intent.category.LAUNCHER" />
           </intent-filter>
       </activity>
    </application>


</manifest>